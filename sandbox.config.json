{
  "allowedEndpoints": [
    "api.anthropic.com",
    "registry.npmjs.org",
    "registry.npmmirror.com"
  ],
  "gitStagingRepo": "/data/git-staging.git",
  "rules": {
    "allow": [
      "exec(echo hello)",
      "packages(bun install test-e2e-pkg)"
    ],
    "deny": [
      "*.datadoghq.com",
      "network(*.datadoghq.com)"
    ]
  },
  "categories": {
    "network": {
      "mode": "approve-each"
    },
    "filesystem": {
      "mode": "approve-each"
    },
    "git": {
      "mode": "approve-each"
    },
    "packages": {
      "mode": "approve-each"
    },
    "sandbox": {
      "mode": "approve-each"
    },
    "exec": {
      "mode": "approve-each"
    }
  }
}
